@model string[]

@{
    ViewData["Title"] = "Configuration Page";
}

<div class="row">
    <div class="col-md-12">
        <table>
            <thead>
            <tr>
                <th>Element</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var element in Model)
            {
                <tr>
                    <td>@element</td>

                </tr>
            }
            </tbody>
        </table>
    </div>
</div>